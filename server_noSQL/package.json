{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node app.js",
    "dev": "nodemon app.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.0",
    "googleapis": "^133.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^8.2.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "netlify-lambda": "^2.0.16",
    "nodemailer": "^6.9.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "serverless-http": "^3.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "no-underscore-dangle": [
        "off"
      ]
    }
  }
}
